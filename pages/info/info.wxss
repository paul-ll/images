.index_box1 .index_box1_auto button image{display:inline-block;width:20px;height:20px;margin-right:2px;  vertical-align:middle; }
.index_box1 .index_box1_left{width:45%;float:left;margin-left:10px;}
.index_box1 .index_box1_left button{float:left;margin-top:18px;width:120px;height:34px;line-height:34px;color:#fff;font-size:16px;background: #FF8601;}
.index_box1 .index_box1_left button image{display:inline-block;width:20px;height:20px;margin-right:2px;  vertical-align:middle; }
.index_box1 .index_box1_right{width:45%;float:right;margin-right:10px;}
.index_box1 .index_box1_right button{float:right;margin-top:18px;width:120px;height:34px;line-height:34px;color:#fff;font-size:16px;background: #FF8601;}
.index_box1 .index_box1_right button image{display:inline-block;width:20px;height:20px;margin-right:2px;  vertical-align:middle;}
